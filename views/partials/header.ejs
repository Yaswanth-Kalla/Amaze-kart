    <link rel="icon" href="https://p7.hiclipart.com/preview/1022/32/945/shopping-cart-logo-icon-shopping-cart.jpg" type="image/x-icon">
    <nav class="navbar bg-body-tertiary">
      <div class="container-fluid">
        <a class="navbar-brand" href="/">
          <img src="https://www.shutterstock.com/shutterstock/photos/1690453492/display_1500/stock-vector-shopping-cart-vector-icon-flat-design-isolated-on-white-background-1690453492.jpg" alt="Logo" width="30" height="24" class="d-inline-block align-text-top">
          Amaze-kart
        </a>

        <div class="dropdown categories">
          <button class="btn btn-secondary dropdown-toggle btn-outline-dark" type="button" data-bs-toggle="dropdown" aria-expanded="false">
            Categories
          </button>
          <div class="dropdown-menu">
            <a class="dropdown-item" href="/items/Mobiles">Mobiles</a>
            <a class="dropdown-item" href="/items/Laptops">Laptops</a>
            <a class="dropdown-item" href="/items/Mobile-Accessories">Mobile Accessories</a>
            <a class="dropdown-item" href="/items/Air-Conditioners">Air Conditioners</a>
            <a class="dropdown-item" href="/items/Televisions">Televisions</a>
            <a class="dropdown-item" href="/items/Washing-Machines">Washing Machines</a>
          </div>
        </div>
        <div class="search">
          <form class="form-inline search-form" action="/" method="post">
            <input type="search"  id="search" name="search_bar" placeholder="Search your product">
              <button class="btn btn-outline-dark search-btn btn-sm" type="submit">
                  <i class="fas fa-search fa-md"></i> 
              </button>
          </form>
        </div>

        <div class="dropdown">
          <button class="btn btn-light dropdown-toggle circular-button" type="button" data-bs-toggle="dropdown" aria-expanded="false">
            <img class="circular-image" src="data:image/png;base64,">
          </button>
          <ul class="dropdown-menu">
            <li><a href="/login" class="login-button">
              <button type="submit" class="btn">Login</button>
            </a></li>
            <li><a href="/register" class="register-button">
              <button type="submit" class="btn">Register</button>
            </a></li>
            <li><a href="/logout" class="logout-button">
              <button type="submit"class="btn">logout</button>
            </a></li>
          </ul>
        </div>

        <a href="/cart" style="color: black;">
          <img src="https://cdn-icons-png.flaticon.com/512/1413/1413908.png" <img style = "width: 24px;">
        </a>
      </div>
    </nav>
</head>

